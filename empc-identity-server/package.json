{
  "name": "empc-identity",
  "version": "2.0.0",
  "description": "servers",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test:dev": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:": "npm run pretest && npm run test:dev",
    "posttest": "npm run lint",
    "test:js": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t empc-identity .",
    "docker:run": "docker run -p 3000:3000 -d empc-identity",
    "sudo:docker:build": "sudo docker build -t empc-identity .",
    "sudo:docker:run": "sudo docker run -p 3000:3000 -d empc-identity",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:watch:dev": "tsc-watch --build --onSuccess \"node .\"",
    "start:watch:test": "tsc-watch --build --onSuccess \"node . --test\"",
    "prepublishOnly": "npm run test",
    "empc:test:dev": "mocha--recursive --exit"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.3.3",
    "@loopback/authorization": "^0.4.10",
    "@loopback/boot": "^1.7.4",
    "@loopback/context": "^2.1.1",
    "@loopback/core": "^1.12.4",
    "@loopback/openapi-v3": "^2.0.0",
    "@loopback/repository": "^1.19.1",
    "@loopback/rest": "^2.0.0",
    "@loopback/rest-explorer": "^1.4.10",
    "@loopback/service-proxy": "^1.3.17",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.11",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/supertest": "^2.0.8",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^5.2.3",
    "mocha": "^10.2.0",
    "moment": "^2.24.0",
    "shortid": "^2.2.15",
    "supertest": "^4.0.2",
    "tsc-watch": "^4.2.3",
    "typescript-logging": "^0.6.4"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.6",
    "source-map-support": "^0.5.16",
    "@loopback/testlab": "^1.10.3",
    "@types/node": "^10.17.14",
    "@typescript-eslint/parser": "^2.19.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@loopback/eslint-config": "^5.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.2.2",
    "typescript": "~3.7.5"
  }
}
